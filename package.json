  "description": "Starter for Angular with TypeScript, npm, Webpack",
  "main": "index.js",
  "scripts": {
    "test": "karma start karma.conf.js",
    "clean": "rimraf build",
    "build:debug": "npm run clean && webpack --config ./config/debug/webpack.config.js",
    "build:release": "npm run clean && webpack --config ./config/release/webpack.config.js",
    "build": "npm run build:debug",
    "start:debug": "webpack-dev-server --config ./config/debug/webpack.config.js",
    "start:release": "webpack-dev-server --config ./config/release/webpack.config.js",
    "start": "npm run start:debug"
  },
